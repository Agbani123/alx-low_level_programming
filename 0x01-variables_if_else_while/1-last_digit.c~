#include <stdlib.h>
#include <time.h>
/* more headers goes here */
#include <stdio.h>

/* betty style doc for function main goes there */
/**
 * main- assign a number each time program is executed
 *Description: Last digit of number n is printed
 *return: always zero
 */

int main(void)
{
int n;
int a;
srand(time(0));
n = rand() - RAND_MAX / 2;
/* your code goes here */
a = n % 10;
if (a > 5)
printf("Last digit of %d is %d and is greater than 5", n, a);
if (a == 0)
printf("Last digit of %d is %d and is 0", n, a);
if (a < 6)
printf("Last digit of %d is %d and is less than 6 and not 0", n, a);
return (0);
}
